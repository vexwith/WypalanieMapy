[node name="Label" parent="Messages/Message3" index="1"]
offset_top = -118.833
offset_bottom = 294.167
text = "Słyszałem wszystko co mówiłeś Narratorze.
Wydaję mi się, że całe to ustrojstwo przyciągnęło mnie z powrotem na wyspę skarbów. W końcu to tu się urodziłem.
Po co komu cofanie w czasie, które jest tak bezużyteczne? Phi"

[node name="Message4" parent="Messages" instance=ExtResource("11_gk7uu")]
layout_mode = 0
offset_left = 1276.0
offset_top = 269.0
offset_right = 1337.0
offset_bottom = 330.0
