[node name="TextureRect" parent="Messages/Message4" index="0"]
offset_left = -204.0
offset_top = -139.0
offset_right = 265.0
offset_bottom = 205.0

[node name="Label" parent="Messages/Message4" index="1"]
offset_top = -98.833
offset_bottom = 255.167
text = "Bardzo mi miło.
Powiem ci jeno, że mimo, że nigdy Cię nie widzieliśmy byłeś dla nas prawdziwym przyjacielem.
Zostałeś z nami do usłanej śmierci.
...Czas odwzajemnić przysługę."

[node name="Message5" parent="Messages" instance=ExtResource("11_gk7uu")]
layout_mode = 0
offset_left = 1442.0
offset_top = 426.0
offset_right = 1503.0
offset_bottom = 487.0
script = ExtResource("12_b2nvl")

[node name="Label" parent="Messages/Message5" index="1"]
offset_top = -78.833
offset_bottom = 216.167
text = "Reksio i reszta poczekają.
Nigdzie nam nie uciekną.
W końcu to pies, nie zając.
Na pewno chcieliby dla odmiany    zobaczyć trochę Twoich przygód."

[node name="Message6" parent="Messages" instance=ExtResource("11_gk7uu")]
layout_mode = 0
offset_left = 875.0
offset_top = 765.0
offset_right = 936.0
offset_bottom = 826.0
script = ExtResource("12_b2nvl")

[node name="Label" parent="Messages/Message6" index="1"]
text = "Oj Narratorze, nigdy nie ma niczego! Pojawiłem się tu w końcu ja i szanowny gracz. To znaczy, że nie widziałeś jeszcze wszystkiego!"

[node name="Message7" parent="Messages" instance=ExtResource("11_gk7uu")]
layout_mode = 0
offset_left = 327.0
offset_top = 307.0
offset_right = 388.0
offset_bottom = 368.0
script = ExtResource("12_b2nvl")

[node name="Label" parent="Messages/Message7" index="1"]
offset_top = -78.833
offset_bottom = 216.167
text = "Jeśli cofanie się w przeszłość
nie działa, spróbujmy przesypać piaski czasu do przodu.
Kto wie - a nuż z tej kosmicznej papki zrodzi się coś nowego?"

[node name="Message8" parent="Messages" instance=ExtResource("11_gk7uu")]
layout_mode = 0
offset_left = 635.0
offset_top = 303.0
offset_right = 696.0
offset_bottom = 364.0
script = ExtResource("12_b2nvl")

[node name="Label" parent="Messages/Message8" index="1"]
offset_top = -18.833
offset_bottom = 217.167
text = "Może czekają nas nowe przygody, albo przynajmniej jakiś dżem."

[node name="Message9" parent="Messages" instance=ExtResource("11_gk7uu")]
